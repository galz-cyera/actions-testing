{
  "name": "aws-s3-scanner",
  "version": "0.9.0-dev.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "npm run clean",
    "build:webpack": "export NODE_OPTIONS=--max_old_space_size=4096 && nest build --webpack --webpackPath ../webpack.config.js",
    "build:nest": "nest build",
    "build": "if [ \"$IS_OBFUSCATION_NEEDED\" = true ]; then npm run build:webpack; else npm run build:nest; fi;",
    "build:test": "npm run build && npm run test",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug 0.0.0.0:9232 --watch",
    "start:debug:webpack": "nest start --debug 0.0.0.0:9232 --webpack --webpackPath ../webpack.config.js --watch",
    "start:fromDist": "node dist/main",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test-ci": "jest",
    "clean": "rimraf dist",
    "fix": "eslint --ext .js,.ts,.tsx src --fix"
  },
  "dependencies": {
    "@aws-sdk/client-iam": "^3.160.0",
    "@aws-sdk/client-s3": "^3.160.0",
    "@aws-sdk/util-arn-parser": "^3.55.0",
    "@nestjs/common": "^9.0.7",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^9.0.7",
    "@nestjs/microservices": "^9.0.7",
    "@nestjs/platform-express": "^9.0.7",
    "@nestjs/typeorm": "^8.1.2",
    "@types/source-map-support": "^0.5.4",
    "ajv": "^8.11.0",
    "ballvalve": "^3.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "common": "^0.8.0-dev.0",
    "data-collector-common": "^0.8.0-dev.0",
    "fast-json-stable-stringify": "^2.1.0",
    "file-processing-common": "^0.8.0-dev.0",
    "jest-junit": "^14.0.0",
    "kafka-common": "^0.8.0-dev.0",
    "lodash": "^4.17.21",
    "nest-common": "^0.8.0-dev.0",
    "p-limit": "^3.1.0",
    "prettier-config": "^0.8.0-dev.0",
    "py-exec": "^0.8.0-dev.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "ts-mixer": "^6.0.1",
    "typeorm": "^0.3.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/testing": "^9.0.7",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "javascript-obfuscator": "^4.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^1.2.0",
    "node-loader": "^2.0.0",
    "prettier": "^2.4.1",
    "test-common": "^0.8.0-dev.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.6.3",
    "webpack-obfuscator": "^3.5.1"
  },
  "jest": {
    "displayName": "aws-s3-scanner",
    "rootDir": "src",
    "testRegex": "/__tests__/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "maxConcurrency": 10,
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testTimeout": 30000,
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "outputDirectory": "../",
    "outputName": "aws-s3-scanner-junit.xml",
    "titleTemplate": "{title}",
    "classNameTemplate": "{classname}",
    "suiteNameTemplate": "{filepath}"
  }
}
